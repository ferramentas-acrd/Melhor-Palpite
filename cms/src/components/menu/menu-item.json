{
  "collectionName": "components_menu_menu_items",
  "info": {
    "displayName": "Menu Item",
    "description": "Item de menu com suporte a subitens"
  },
  "options": {},
  "attributes": {
    "label": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true
    },
    "icon": {
      "type": "string",
      "required": false
    },
    "target": {
      "type": "enumeration",
      "enum": [
        "_self",
        "_blank",
        "_parent",
        "_top"
      ],
      "default": "_self"
    },
    "children": {
      "type": "component",
      "repeatable": true,
      "component": "menu.menu-item"
    },
    "active": {
      "type": "boolean",
      "default": true
    }
  }
}
{
  "kind": "collectionType",
  "collectionName": "menu_navigations",
  "info": {
    "singularName": "menu-navigation",
    "pluralName": "menu-navigations",
    "displayName": "Menu Navigation",
    "description": "Gerenciamento de menus do site"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "header",
        "footer",
        "sidebar"
      ],
      "required": true
    },
    "items": {
      "type": "component",
      "repeatable": true,
      "component": "menu.menu-item"
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}